			"cStandard": "gnu17",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "windows-gcc-x86"
		},
		{
			"name": "mingw64",
			"includePath": [
				"${workspaceFolder}/**",
				"${LocalAppData}/Qt/5.12.11/mingw73_64/include/**"
			],
			"defines": [
				"NDEBUG"
			],
			"compilerPath": "${LocalAppData}/Qt/Tools/mingw730_64/bin/gcc.exe",
			"cStandard": "gnu17",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "windows-gcc-x64"
