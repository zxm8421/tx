{
	"configurations": [
		{
			"name": "linux",
			"includePath": [
				"${workspaceFolder}/**",
				"${HOME}/Apps/Qt/5.12.11/gcc_64/include/**"
			],
			"defines": [
				"NDEBUG"
			],
			"compilerPath": "/usr/bin/gcc",
			"cStandard": "gnu17",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "${default}"
		},
		{
			"name": "mingw32",
			"includePath": [
				"${workspaceFolder}/**",
				"${LocalAppData}/Qt/5.12.11/mingw73_32/include/**"
